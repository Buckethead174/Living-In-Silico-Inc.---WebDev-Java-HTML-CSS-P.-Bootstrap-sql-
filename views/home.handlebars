<div class="label">
            <img class="logo" src="../public/Photos/living.jpg" alt="Company Logo">
            
            <span class="company-Label">
                Living In Silico
            </span> 
        </div>
        <div class = "testing">
            <div class = "parameters">
                <div id = "paremetersBasic">
                    <div class="content1">
                        <span class = "param_text">
                            Parameters
                        </span>
                        <form class="basicInput" action="basic" method="post" enctype="multipart/form-data">
                            <label for="receptor" class="receptor">Receptor:</label>
                            <div class="contentBorder">
                            <input type="file" id="receptor" name='receptor' required
                            accept=".pdbqt, .pdb, .sdf">
                            </div>

                            <br/>
                            <label for="ligand" class="ligand">Ligand:</label>
                            <div class="contentBorder">
                            <input type="file" id="ligand" name='ligand' required
                            accept=".pdbqt, .pdb, .sdf" multiple>
                            </div>

                            <br/>
                            <label for="xCenter" class="center">Box Center: X, Y, Z</label>
                            <div class="textInput">
                                <input type="text" id="xCenter" name="xCenter" required value="0">
                                <input type="text" id="yCenter" name="yCenter" required value="0">
                                <input type="text" id="zCenter" name="zCenter" required value="0">
                            </div>

                            <br/>
                            <label for="xBox" class="Box">Box size: X, Y, Z</label>
                            <div class="textInput">
                                <input type="text" id="xBox" name="xBox" required value="0">
                                <input type="text" id="yBox" name="yBox" required value="0">
                                <input type="text" id="zBox" name="zBox" required value="0">
                            </div>

                            <br/>
                            <label for="cpu" class="cpu">CPUs:</label>
                            <div class="textInput">
                                <input type="number" id="cpu" name="cpu" required>
                            </div>

                            <br/>
                            <!--
                            <label for="test" class="test">Test:</label>
                            <input type="number" id="test" name="test" required>
                            -->
                            
                            <label for="exhaust" class="exhaust">Exhaustivness:</label>
                            <div class="textInput">
                                <input type="number" id="exhaust" name="exhaust" required>
                            </div>
                            <br>
                            <input type="submit">
                        </form>
                    </div>
                </div>
            </div>

            <div class = "docking">
                <div class = "docking_words">
                    Docking Box Output
                </div>
                <div>
                    <a href="/download" class="button">Download</a>
                    <div id="downloadText">
                        {{Download}}
                    </div>
                    <textarea class="scrollBox" readonly>
                        {{txtOut}}
                    </textarea>
                </div>
            </div>

            <div class = "run">
                <div class="sminaBox">
                <form class="SminaRun" action="run" method="get">
                    <input type="submit">
                </form>
                <span class="Vina"> Vina Command:</span>
                </div>
                <textarea id="Vina_text" readonly>{{SminaLine}}</textarea>
                <p>Completion: <span id="sminaFin">{{finish}}</span></p>
            </div>
        </div>
        <script src="index.js">
        </script>